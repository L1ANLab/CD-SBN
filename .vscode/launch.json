{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceRoot}/build/cdsbn",
            "stopAtEntry": true, 
            "cwd": "${workspaceRoot}",
            "environment": [],
            "args": [
                "-i", "dataset/BS/initial_graph.txt",
                "-l", "dataset/BS/label_list.txt",
                "-u", "dataset/BS/update_stream.txt",
                "-q", "dataset/BS/query_keywords_list-5.txt",
                "-t", "599996400",
                "-w", "100",
                "-k", "4",
                "-r", "2",
                "-s", "2"
            ],
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "externalConsole": false,
            "MIMode": "gdb",
            "preLaunchTask": "build"
        }
    ]
}